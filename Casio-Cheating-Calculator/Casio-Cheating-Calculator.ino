//This function sets voltage levels of the GPIOs that are connected to the SN74LVC1G3157 and 74HC4051 chips, in order emulate a button press.
void digitalWriteToPins() {
  if (is0) {
    digitalWrite(is0pin, HIGH);
  }
  else {
    digitalWrite(is0pin, LOW);
  }
  if (is1) {
    digitalWrite(is1pin, HIGH);
  }
  else {
    digitalWrite(is1pin, LOW);
  }
  if (is2) {
    digitalWrite(is2pin, HIGH);
  }
  else {
    digitalWrite(is2pin, LOW);
  }
  if (os0) {
    digitalWrite(os0pin, HIGH);
  }
  else {
    digitalWrite(os0pin, LOW);
  }
  if (os1) {
    digitalWrite(os1pin, HIGH);
  }
  else {
    digitalWrite(os1pin, LOW);
  }
  if (os2) {
    digitalWrite(os2pin, HIGH);
  }
  else {
    digitalWrite(os2pin, LOW);
  }
  if (usedelayflag) {
    //By setting the S input of the SN74LVC1G3157 to LOW and HIGH after, the connection between the selected KIx & KOx is established, hence the button press is simulated.
    //The 50ms delays are used between these 'digitalWrite's to ensure the fx-82MS can detect the desired button press.
    digitalWrite(spin, LOW);
    delay(50);
    digitalWrite(spin, HIGH);
    delay(50);
  }
  else {
    emulatebuttonpressflag=true;
  }
}
