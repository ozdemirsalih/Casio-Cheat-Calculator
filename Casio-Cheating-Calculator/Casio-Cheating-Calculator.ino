      function onMessage(event) {
        if (event.data instanceof ArrayBuffer) {
          dataviewforbinarymessage=new DataView(event.data);
          accelbinarymessagelength=dataviewforbinarymessage.getUint8(0);
          for (j=1; j<accelbinarymessagelength; j+=6) {
            accelodrcount++;
            accelxlsb=dataviewforbinarymessage.getUint8(j);
            accelxmsb=dataviewforbinarymessage.getUint8(j+1);
            accelylsb=dataviewforbinarymessage.getUint8(j+2);
            accelymsb=dataviewforbinarymessage.getUint8(j+3);
            accelzlsb=dataviewforbinarymessage.getUint8(j+4);
            accelzmsb=dataviewforbinarymessage.getUint8(j+5);
            accelx=((accelxmsb*256)+(accelxlsb&0xF0))/16;
            if (accelx>2047) {
              accelx-=4096;
            }
            accely=((accelymsb*256)+(accelylsb&0xF0))/16;
            if (accely>2047) {
              accely-=4096;
            }
            accelz=((accelzmsb*256)+(accelzlsb&0xF0))/16;
            if (accelz>2047) {
              accelz-=4096;
            }
            arrayforaccelx.shift();
            arrayforaccelx.push(accelx);
            arrayforaccely.shift();
            arrayforaccely.push(accely);
            arrayforaccelz.shift();
            arrayforaccelz.push(accelz);
          }
        }
        else {
          if (event.data=='~') {
            latency2=performance.now();
            latencylabel.innerHTML='';
            if (Math.floor((latency2-latency1)/2)<10000) {
              latencylabel.innerHTML=': '+Math.floor((latency2-latency1)/2)+'ms';
            }
            else {
              latencylabel.innerHTML=': ---';
            }
          }
          else {
            if (event.data.charAt(0)=='r') {
              if (event.data.charAt(1)=='a') {
                accelconfigmessage=event.data;
                accelconfigmessagearray=event.data.substring(2).split('-');
                populateconfigswithaccelconfigmessagearray();
              }
              else if (event.data.charAt(1)=='g') {
                //Config data for gyroscope has been read...
              }
              else if (event.data.charAt(1)=='m') {
                //Config data for magnetometer has been read...
              }
            }
            else if (event.data.charAt(0)=='w') {
              if (event.data.charAt(1)=='a') {
                accelconfigmessage=event.data;
                accelconfigmessagearray=event.data.substring(2).split('x');
                populateconfigswithaccelconfigmessagearray();
              }
              else if (event.data.charAt(1)=='g') {
                //Config data for gyroscope has been written...
              }
              else if (event.data.charAt(1)=='m') {
                //Config data for magnetometer has been written...
              }
            }
            else {
              console.log('Unidentified data has been received: '+event.data);
            }
          }
        }
      }
