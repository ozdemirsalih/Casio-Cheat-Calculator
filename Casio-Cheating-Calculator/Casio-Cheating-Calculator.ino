      function step(timestamp) {
        if (animation_start===undefined) {
          animation_start=timestamp;
        }
        animation_elapsed=timestamp-animation_start;
        animation_percentage=animation_elapsed/animation_treshold;
        if (animation_elapsed>animation_treshold) {
          nod_animation_counter=0;
          element_to_animate.style.transform='matrix(1, 0, 0, 1, 0, 0)';
          animation_start=undefined;
        }
        else {
          nod_animation_counter++;
          if ((nod_animation_counter%12)==0) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, -1, 0)';
          }
          else if ((nod_animation_counter%12)==1) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, -2, 0)';
          }
          else if ((nod_animation_counter%12)==2) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, -3, 0)';
          }
          else if ((nod_animation_counter%12)==3) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, -2, 0)';
          }
          else if ((nod_animation_counter%12)==4) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, -1, 0)';
          }
          else if ((nod_animation_counter%12)==5) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 0, 0)';
          }
          else if ((nod_animation_counter%12)==6) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 1, 0)';
          }
          else if ((nod_animation_counter%12)==7) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 2, 0)';
          }
          else if ((nod_animation_counter%12)==8) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 3, 0)';
          }
          else if ((nod_animation_counter%12)==9) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 2, 0)';
          }
          else if ((nod_animation_counter%12)==10) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 1, 0)';
          }
          else if ((nod_animation_counter%12)==11) {
            element_to_animate.style.transform='matrix(1, 0, 0, 1, 0, 0)';
          }
          window.requestAnimationFrame(step);
        }
      }
      function nodElementAnimate(elementid) {
        if (animation_start===undefined) {
          element_to_animate=document.getElementById(elementid);
